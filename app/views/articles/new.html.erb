<div class="container">
    <h1>New Article</h1>

    <%= form_for :article, url: articles_path do |f| %>
        <p>
        <%= f.label :title %> </br>
        <%= f.text_field :title %>
        </p>

        <p>
        <%= f.label :link %> </br>
        <%= f.text_field :link %>
        </p>

        <button class="waves-effect waves-light btn blue">
        <%= f.submit %>
        </button>
    <% end %>
</div>
